		}
	}
	else
	{
		new iCounter = floatround(floatmul(1088421888, floatmul(-g_flVelocity[id][2], g_flFrameTime[id])), 1);
		while (!fm_trace_hull(g_flJumpbugLand[id], 1, id, 0) && iCounter > 0)
		{
			new var9 = g_flJumpbugLand[id][2];
			var9 = floatsub(var9, 1065353216);
			iCounter--;
		}
		if (0 >= iCounter)
		{
			return 0;
		}
		new var10 = g_flJumpbugLand[id][2];
		var10 = floatadd(1065353216, var10);
		new Float:flDistance = floatsub(float(floatround(g_flLastGround[id][2], xs__ITaskId)), g_flJumpbugLand[id][2]);
		if (flDistance <= 0.0)
		{
			return 0;
		}
		new Float:flUnitsAbove = floatsub(g_flJumpDuck[id][2], g_flJumpbugLand[id][2]);
		if (flUnitsAbove <= 0.0)
		{
			return 0;
		}
		if (flUnitsAbove > 2.0)
		{
			g_iWaitForGoodFrame[id] = 40;
		}
		else
		{
			jumpBugDone(id, flDistance, g_flJumpDuck[id][2], g_flJumpbugLand[id][2]);
		}
	}
	return 0;
}

public findGoodFrame(id)
{
	g_iWaitForGoodFrame[id]--;
	if (g_iWaitForGoodFrame[id])
	{
		if (!g_iFlags[id] & 512)
		{
			return 0;
		}
		if (40 > ArraySize(g_aOrigins[id]))
		{
			return 0;
		}
		new Float:flDistance = floatsub(float(floatround(g_flLastGround[id][2], xs__ITaskId)), g_flJumpbugLand[id][2]);
		new iFramesShift = 40 - g_iWaitForGoodFrame[id];
		g_iWaitForGoodFrame[id] = 0;
		new pos = 39;
		new Float:flCorrOrigin = ArrayGetCell(g_aOrigins[id], pos);
		new Float:flPrevOrigin = ArrayGetCell(g_aOrigins[id], pos + -1);
		new Float:flPredictedOrigin = floatadd(flPrevOrigin, floatmul(floatsub(g_flPrevVelocity[id][2], floatmul(1137180672, g_flPrevFrameTime[id])), g_flPrevFrameTime[id]));
		if (flPredictedOrigin < flCorrOrigin)
		{
			jumpBugNotPoss(id, flDistance, g_flJumpDuck[id][2], g_flJumpbugLand[id][2], flPrevOrigin, iFramesShift + -1, flCorrOrigin, 999, false);
		}
		else
		{
			jumpBugFailed(id, flDistance, g_flJumpDuck[id][2], g_flJumpbugLand[id][2], flPredictedOrigin, iFramesShift, false);
		}
		return 0;
	}
	return 0;
}

